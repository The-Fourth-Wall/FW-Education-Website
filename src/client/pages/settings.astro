---
import Text from "@atoms/text/Text.astro";
import BackButton from "@molecules/BackButton.astro";
import Layout from "@templates/Layout.astro";
---

<Layout title="Settings Page">
  <dev>
    <Text
      label="Settings Page"
      size={1.5}
      type="primary"
      color="var(--white)"
    />

    <BackButton href="/" />
  </dev>
</Layout>

<style>
  dev {
    display: flex;
    justify-content: space-between;
  }
</style>
